{
  "name": "Nuxt-My-Starter-Kit",
  "version": "1.0.0",
  "description": "Let's Begin Now!",
  "author": "kojiyamauchi",
  "private": true,
  "dependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/auth": "^4.8.0",
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@types/node": "^12.0.10",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "firebase": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "1.3.7",
    "husky": "^2.4.1",
    "imports-loader": "^0.8.0",
    "json-server": "^0.15.0",
    "lint-staged": "^8.2.1",
    "npm-check-updates": "^3.1.11",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.8.1",
    "nuxt-class-component": "^1.3.0",
    "nuxt-device-detect": "^1.1.5",
    "nuxt-property-decorator": "^2.3.0",
    "nuxt-stylus-resources-loader": "^2.0.0",
    "opener": "^1.5.1",
    "prettier": "^1.18.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.2",
    "vue-mixin-decorator": "^1.1.1",
    "vuefire": "^2.0.0",
    "vuex-class": "^0.3.2",
    "vuex-persistedstate": "^2.5.4",
    "vuex-type-helper": "^1.2.2",
    "vuexfire": "^3.0.0",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{vue,ts,js}": [
        "prettier --write --no-semi --single-quote --print-width 160 --jsx-bracket-same-line true",
        "git add"
      ]
    },
    "ignore": []
  },
  "scripts": {
    "server": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "browser": "wait-on tcp:3000 && opener -a 'Google Chrome' https://localhost:3000",
    "dev": "npm-run-all -p server browser",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "deploy:github_pages": "cross-env NODE_ENV=GITHUB_PAGES nuxt generate",
    "check-pkg": "ncu",
    "update-pkg": "ncu -a"
  }
}
