{
  "name": "Nuxt-My-Starter-Kit",
  "version": "1.0.0",
  "description": "Let's Begin Now!",
  "author": "kojiyamauchi",
  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^2.6.0",
    "@types/node": "^10.12.21",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "husky": "^1.3.1",
    "imports-loader": "^0.8.0",
    "json-server": "^0.14.2",
    "lint-staged": "^8.1.3",
    "npm-check-updates": "^2.15.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.4.3",
    "nuxt-class-component": "^1.2.1",
    "nuxt-device-detect": "^1.1.3",
    "nuxt-property-decorator": "^1.3.1",
    "nuxt-stylus-resources-loader": "^2.0.0",
    "opener": "^1.5.1",
    "prettier": "^1.16.4",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.3.1",
    "vue-mixin-decorator": "^1.1.0",
    "vuex-class": "^0.3.1",
    "vuex-type-helper": "^1.2.0",
    "wait-on": "^3.2.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{vue,ts,js}": [
        "prettier --write --no-semi --single-quote --print-width 160 --jsx-bracket-same-line true",
        "git add"
      ]
    },
    "ignore": []
  },
  "scripts": {
    "server": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "browser": "wait-on tcp:3000 && opener -a 'Google Chrome' https://localhost:3000",
    "dev": "npm-run-all -p server browser",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "deploy:github_pages": "cross-env NODE_ENV=GITHUB_PAGES nuxt generate",
    "check-pkg": "ncu",
    "update-pkg": "ncu -a"
  }
}
